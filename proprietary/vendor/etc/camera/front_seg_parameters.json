    "PostProcessList": [
        {
            "head_type": "Segmentation",
            "head_id": "seg_head",
            "head_dep": [
                "Executor",
                "PreProcess"
            ],
            "param": {
                "logit_map": true,
                "output_channel": 1,
                "scaler": 255

            }
        }
    ]
}
