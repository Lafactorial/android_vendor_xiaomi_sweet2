{
    "Loader_param": {
        "type": "segmentation",
        "mode": "normal",
        "name": "front_portrait_blur"
    },
    "PreProcess": {
        "inputs": [
            {
                "name": "input",
                "shape": [1, 256, 256, 3]
            }
        ],
        "outputs": [
            {
                "name": "output",
                "shape": [1, 256, 256, 2]
            }
        ],
        "resize_keep_ratio": false,
        "image_mean": [
            0.0,
            0.0,
            0.0
        ],
        "image_std": [
            1.0,
            1.0,
            1.0
        ],
        "inputimage_format": "bgr",
        "size_divisor": 32,
        "do_pixel_norm": false
    },
    "Executor": {
        "model_folder": "proposal_final",
        "adsp_path": "/vendor/lib/rfsa/adsp;/system/lib/rfsa/adsp;/system/vendor/lib/rfsa/adsp;/dsp",
        "asset_path": "/vendor/etc/camera",
        "log_keyword":"algo_seg",
        "verify":"mialgo-multicam",
        "reserved_string":"",
        "reserved_int":0
